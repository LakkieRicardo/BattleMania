    {
        "id": "manage_punish_temp",
        "title": "Manage User",
        "layout_rows": [
            "M00R0T000",
            "0C00E00L0",
            "010040060",
            "020050070",
            "030000000",
            "B0000000N"
        ],
        "definitions": {
            "0": {
                "type": "PROP",
                "material": "GRAY_STAINED_GLASS_PANE",
                "name": "&0"
            },
            "T": {
                "type": "PLAYER_HEAD",
                "owner": "$Target",
                "name": "&aManaging <Target.Username>",
                "lore": [
                    "&7UUID: &c<Target.UUID>",
                    "&7Online: &7&l<Target.Online.YesNo>",
                    "&7Ingots: &c<Target.Ingots>",
                    "&7Tokens: &c<Target.Tokens>",
                    "&7Time: &c<Target.IngameHours> hours",
                    "&7Rank: &c<Target.Rank.DisplayName>",
                    "&7Server: &c<Target.Server>"
                ]
            },
            "R": {
                "type": "PLAYER_HEAD",
                "owner": "$Viewer",
                "name": "&aYou (Punisher)",
                "lore": [
                    "&7You will be credited with all punishments",
                    "&7and Helper+ will be notified of the punishment."
                ]
            },
            "C": {
                "type": "PROP",
                "material": "LIME_STAINED_GLASS_PANE",
                "name": "&aChat Punishments",
                "lore": [
                    "&7These punishments are applied for breaking",
                    "&7chat-specific rules."
                ]
            },
            "E": {
                "type": "PROP",
                "material": "YELLOW_STAINED_GLASS_PANE",
                "name": "&aExploit Punishments",
                "lore": [
                    "&7These punishments are applied if the",
                    "&7user is abusing a typically simpler",
                    "&7unfair gameplay mechanic to ruin the",
                    "&7experience of other players."
                ]
            },
            "L": {
                "type": "PROP",
                "material": "RED_STAINED_GLASS_PANE",
                "name": "&aClient Punishments",
                "lore": [
                    "&7These punishments are applied if the",
                    "&7user has downloaded/made a modded client",
                    "&7and is actively using it to cheat."
                ]
            },
            "N": {
                "type": "PROP",
                "material": "REDSTONE_BLOCK",
                "name": "&aGo to Permanent",
                "lore": [
                    "&7View permanent punishments"
                ]
            },
            "M": {
                "type": "PROP",
                "material": "STONE_BUTTON",
                "name": "&aTemporary Punishments",
                "lore": [
                    "&7You are currently viewing temporary punish",
                    "&7Click the bottom-right button to go to",
                    "&7permanent punish."
                ]
            },
            "1": {
                "type": "PROP",
                "material": "GREEN_CONCRETE",
                "name": "&aMute Severity 1",
                "button_id": "Mute",
                "punish_days": 1,
                "lore": [
                    "&7This is for someone who is ruining gameplay for other players",
                    "&7either by posting spam, being toxic, etc. However, this",
                    "&7contrasts with joining the server without playing with the",
                    "&7intent to harass, spam, etc. Those are a higher severity.",
                    "&eNote: &7If this player has not been punished before, use a warning."
                ]
            },
            "2": {
                "type": "PROP",
                "material": "YELLOW_CONCRETE",
                "name": "&aMute Severity 2",
                "button_id": "Mute",
                "punish_days": 7,
                "lore": [
                    "&7This is for a player who has been muted for similar reasons",
                    "&7as Mute Severity 1, but has done it consistently. Use this",
                    "&7as a sign to tell them to take a break from chat.",
                    "&eNote: &7Do not apply this punishment unless severity 1",
                    "&7has been used before."
                ]
            },
            "3": {
                "type": "PROP",
                "material": "RED_CONCRETE",
                "name": "&aMute Severity 3",
                "button_id": "Mute",
                "punish_days": 30,
                "lore": [
                    "&7This is someone who joined the server with the intent to",
                    "&7to ruin the usability of chat for other players. However,",
                    "&7this player is not violating any gameplay rules.",
                    "&eNote: &7This is a significantly longer punishment. Think",
                    "&7before punishing."
                ]
            },
            "4": {
                "type": "PROP",
                "material": "GREEN_CONCRETE",
                "name": "&aBan Severity 1",
                "button_id": "Ban",
                "punish_days": 1,
                "lore": [
                    "&7This is for someone who is abusing a simple loophole or",
                    "&7some sort of bug in order to gain an advantage. This is",
                    "&7for someone who only being doing this occasionally.",
                    "&eNote: &7As always, warn before a ban. Think about banning",
                    "&7before you go through with it. Do not ban players",
                    "&7unnecessarily."
                ]
            },
            "5": {
                "type": "PROP",
                "material": "YELLOW_CONCRETE",
                "name": "&aBan Severity 2",
                "button_id": "Ban",
                "punish_days": 14,
                "lore": [
                    "&7This is for someone who is consistently abusing exploits",
                    "&7to gain a gameplay advantage. This is someone who has been",
                    "&7warned and/or banned before, and needs a longer punishment.",
                    "&eNote: &7Check punishment history before applying."
                ]
            },
            "6": {
                "type": "PROP",
                "material": "GREEN_CONCRETE",
                "name": "&aBan Severity 1",
                "button_id": "Ban",
                "punish_days": 21,
                "lore": [
                    "&7This applies to someone using an obviously modded client",
                    "&7in a way which does not involve combat. This is against",
                    "&7the rules, but will likely not harm other players' game-",
                    "&7play directly.",
                    "&eNote: Check punishment history before applying."
                ]
            },
            "7": {
                "type": "PROP",
                "material": "YELLOW_CONCRETE",
                "name": "&aBan Severity 2",
                "button_id": "Ban",
                "punish_days": 60,
                "lore": [
                    "&7This is someone who is using an obviously modded client",
                    "&7but in a manner which directly affects other players'",
                    "&7gameplay (ex: combat hacks like regen and reach)"
                ]
            },
            "B": {
                "type": "PROP",
                "material": "STONE_BUTTON",
                "name": "&aGo to Home",
                "lore": [
                    "&7Go back to the home page in Manage User"
                ]
            }
        }
    },
